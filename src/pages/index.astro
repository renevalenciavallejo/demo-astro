<script is:inline>
const KNOWN_LANGUAGES = ["en", "es"];
const userLanguage = (window.navigator.language || "en-US").substring(0, 2);
const preferredLanguage = KNOWN_LANGUAGES.includes(userLanguage)
    ? userLanguage
    : "en";
window.location.pathname = "/" + preferredLanguage;
</script>
